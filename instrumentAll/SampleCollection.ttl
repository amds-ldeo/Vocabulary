  skos:inScheme <http://vocab.nerc.ac.uk/scheme/SDNDEV/current> ;
  skos:notation "SDN:L22::NETT0073" ;
  skos:note "accepted"@en ;
  skos:prefLabel "Modified opening-closing Isaacs-Kidd Midwater Trawl - Brown (1975)"@en ;
