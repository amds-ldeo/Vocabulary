  skos:inScheme <http://vocab.nerc.ac.uk/scheme/SDNDEV/current> ;
  skos:notation "SDN:L22::NETT0074" ;
  skos:note "accepted"@en ;
  skos:prefLabel "International Council for the Exploration of the Seas standard net - Ostenfeld and Jespersen  (1924)"@en ;
