  skos:notation "SDN:L22::TOOL0535" ;
  skos:note "accepted"@en ;
  skos:prefLabel "Marine Scotland Opening Closing Environmental Acoustic Net water bottle"@en ;
  skos:related <http://vocab.nerc.ac.uk/collection/B75/current/ORG00013> ;
