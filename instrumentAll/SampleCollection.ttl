inst:NETT0073
  rdf:type skos:Concept ;
  dcterm:date "2011-10-21 14:18:49.0" ;
  dcterm:identifier "SDN:L22::NETT0073" ;
  pav:hasCurrentVersion <http://vocab.nerc.ac.uk/collection/L22/current/NETT0073/1> ;
  owl:versionInfo "1" ;
  skos:altLabel "Brown modified IKMT"@en ;
  skos:broader <http://vocab.nerc.ac.uk/collection/L05/current/23> ;
