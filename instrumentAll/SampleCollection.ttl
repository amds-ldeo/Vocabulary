inst:NETT0162
  rdf:type skos:Concept ;
  dcterm:date "2011-10-21 14:18:49.0" ;
  dcterm:identifier "SDN:L22::NETT0162" ;
  pav:hasCurrentVersion <http://vocab.nerc.ac.uk/collection/L22/current/NETT0162/1> ;
  owl:versionInfo "1" ;
  skos:altLabel "Davis modified Tucker Trawl"@en ;
  skos:broader <http://vocab.nerc.ac.uk/collection/L05/current/23> ;
  skos:definition "Modification used timing clocks to open and close the Tucker trawl mouth.  Net design modified from that described by Tucker so that first 500 cm of the net mesh was 1.1 cm Marlon netting and last 200 cm was 0.33 mm nylon mesh.  A depth-telemetering pinger used to monitor net depth during tow and a depth-time recorder used to make alternate record [Wiebe and Benfield 2003 Plate 9 E]."@en ;
  skos:inScheme <http://vocab.nerc.ac.uk/scheme/SDNDEV/current> ;
  skos:notation "SDN:L22::NETT0162" ;
