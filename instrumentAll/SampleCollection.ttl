  rdf:type skos:Concept ;
  dcterm:date "2022-10-10 14:39:24.0" ;
  dcterm:identifier "SDN:L22::TOOL1853" ;
  pav:hasCurrentVersion <http://vocab.nerc.ac.uk/collection/L22/current/TOOL1853/1> ;
  owl:versionInfo "1" ;
  skos:altLabel "Unspecified Dredge"@en ;
  skos:broader <http://vocab.nerc.ac.uk/collection/L05/current/60> ;
  skos:definition "A type of sampler designed for collecting a sample of bottom substrate, usually by towing it through the substrate. It can be suitable for mud, sand, shell, and gravel substrates. Samples can be used to study benthic macroinvertebrates and algae as well as the chemistry and structure of the sediments themselves."@en ;
  skos:inScheme <http://vocab.nerc.ac.uk/scheme/SDNDEV/current> ;
