  skos:inScheme <http://vocab.nerc.ac.uk/scheme/SDNDEV/current> ;
  skos:notation "SDN:L22::TOOL0588" ;
  skos:note "accepted"@en ;
  skos:prefLabel "Tedlar atmospheric sampling bag"@en ;
